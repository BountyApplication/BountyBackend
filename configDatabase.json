{
    "accounts" : {
        "columns": {
            "userId" : "INTEGER PRIMARY KEY AUTOINCREMENT",
            "firstname" : "VARCHAR(20)",
            "lastname" : "VARCHAR(30)",
            "balance" : "FLOAT",
            "joining" : "DATE",
            "cardId" : "INTEGER"
        },
        "additional" :"FOREIGN KEY (cardId) REFERENCES cards(cardId)"
    },
    "history" : {
        "columns" : {
            "bookingId" : "INTEGER PRIMARY KEY AUTOINCREMENT",
            "userId" : "INTEGER",
            "date" : "DATE",
            "oldBalance" : "FLOAT",
            "newBalance" : "FLOAT",
            "total" : "FLOAT",
            "correction" : "FLOAT",
            "cashPayment" : "FLOAT",
            "productSum" : "FLOAT",
            "products" : "VARCHAR(1500)"
        },
        "additional" : "FOREIGN KEY (userId) REFERENCES accounts(userId)"
    },
    "products" : {
        "columns" : {
            "productId" : "INTEGER PRIMARY KEY AUTOINCREMENT",
            "name" : "VARCHAR(20)",
            "price" : "FLOAT",
            "active" : "INTEGER DEFAULT 1"
        },
        "additional" : ""
    },
    "cards" : {
        "columns" : {
            "cardId" : "INTEGER PRIMARY KEY AUTOINCREMENT",
            "userId" : "INTEGER"
        },
        "additional" :"FOREIGN KEY (userId) REFERENCES accounts(userId)"
    }
}